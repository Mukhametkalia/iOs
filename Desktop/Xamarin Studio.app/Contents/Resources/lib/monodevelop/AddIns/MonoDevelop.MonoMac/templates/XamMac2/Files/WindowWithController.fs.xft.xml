<?xml version="1.0"?>
<Template>
	<TemplateConfiguration>
		<_Name>Window Controller</_Name>
		<Icon>xib-template-window</Icon>
		<_Category>Mac</_Category>
		<LanguageName>F#</LanguageName>
		<DefaultFilename>Window</DefaultFilename>
		<ProjectType>XamMac2</ProjectType>
		<_Description>A Window with a Controller for Xamarin.Mac.</_Description>
	</TemplateConfiguration>

	<TemplateFiles>
		<File name="${Name}.xib" src="WindowWithController/Window.xib" DefaultExtension=".xib" SuppressAutoOpen="True"/>
		<UnformattedFile name="${Name}.fs" src="WindowWithController/F#/Window.fs.txt"/>
		<UnformattedFile name="${Name}Controller.fs" src="WindowWithController/F#/WindowController.fs.txt"/>
	</TemplateFiles>
</Template>