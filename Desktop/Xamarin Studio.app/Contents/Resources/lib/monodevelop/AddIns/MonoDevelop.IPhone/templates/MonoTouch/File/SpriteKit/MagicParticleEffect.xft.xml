<?xml version="1.0" encoding="UTF-8" ?>
<Template>
	<TemplateConfiguration>
		<_Name>Magic</_Name>
		<Icon>xib-template-view</Icon>
		<_Category>iOS SpriteKit Particle Effects</_Category>
		<LanguageName>*</LanguageName>
		<ProjectType>MonoTouch</ProjectType>
		<DefaultFilename>MagicParticleEffect</DefaultFilename>
		<_Description>Creates a magic particle effect.</_Description>
	</TemplateConfiguration>
	
	<Conditions>
		<ParentProject RequireProject="True"/>
	</Conditions>
	
	<TemplateFiles>
		<RawFile name="${Name}.sks" SuppressAutoOpen="True" src="MagicParticleEffect.sks" />
		<RawFile name="spark.png" SuppressAutoOpen="True" src="spark.png" />
    </TemplateFiles>
</Template>
