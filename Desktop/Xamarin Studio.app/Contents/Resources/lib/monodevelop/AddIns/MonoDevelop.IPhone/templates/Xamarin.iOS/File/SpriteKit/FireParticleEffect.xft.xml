<?xml version="1.0" encoding="UTF-8" ?>
<Template>
	<TemplateConfiguration>
		<_Name>Fire</_Name>
		<Icon>file-spritekit</Icon>
		<_Category>iOS SpriteKit Particle Effects</_Category>
		<LanguageName>*</LanguageName>
		<ProjectType>XamarinIOS</ProjectType>
		<DefaultFilename>FireParticleEffect</DefaultFilename>
		<_Description>A fire particle effect.</_Description>
	</TemplateConfiguration>
	
	<Conditions>
		<ParentProject RequireProject="True"/>
	</Conditions>
	
	<TemplateFiles>
		<RawFile name="${Name}.sks" SuppressAutoOpen="True" src="FireParticleEffect.sks" />
		<RawFile name="spark.png" SuppressAutoOpen="True" src="spark.png" />
    </TemplateFiles>
</Template>
