<?xml version="1.0" encoding="UTF-8" ?>
<Template>
	<TemplateConfiguration>
		<_Name>SpriteKit Action</_Name>
		<Icon>file-spritekit</Icon>
		<_Category>iOS</_Category>
		<LanguageName>*</LanguageName>
		<ProjectType>XamarinIOS</ProjectType>
		<DefaultFilename>MyActions</DefaultFilename>
		<_Description>A simple action template for SpriteKit.</_Description>
	</TemplateConfiguration>

	<Conditions>
		<ParentProject RequireProject="True"/>
	</Conditions>

	<TemplateFiles>
		<RawFile name="${Name}.sks" SuppressAutoOpen="True" src="Action.sks" />
    </TemplateFiles>
</Template>
